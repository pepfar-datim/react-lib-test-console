    {
      "path": "dist/dev-tools.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^4.9.2",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.1",
    "husky": "^5.1.3",
    "parcel": "^2.0.0-beta.1",
    "react": "*",
    "react-dom": "^17.0.1",
    "size-limit": "^4.9.2",
    "tsdx": "^0.14.1",
    "tslib": "^2.1.0",
    "typescript": "^3.9.9"
  },
  "repository": {
    "url": "https://github.com/pepfar-datim/react-lib-test-console",
    "type": "git"
  }
